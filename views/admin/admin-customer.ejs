<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- css -->
    <link rel="stylesheet" href="../public/css/admin/ADMIN.CSS">
    <!-- bootstrap -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.3.0/font/bootstrap-icons.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65" crossorigin="anonymous">
 <!-- ------------fav icon------------------ -->
 <link rel="shortcut icon" href="/public/img/log.png" type="image/x-icon">

                       <!-- font link -->
    <!-- zen dots -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Zen+Dots&display=swap" rel="stylesheet">
    <!-- odibee -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Odibee+Sans&display=swap" rel="stylesheet">
    <title>Document</title>

</head>

<body>   
  <%- include('partials/navadmin') %>


 <div class="container d-flex flex-column"> 
  <% if(locals.project!=0){%>

  <%project.forEach(project=>{ %>
  <div class="box my-2 d-flex border border-light rounded ">
  <div class="row d-flex flex-column mx-2 text-light pt-1">
    <div class="col">NAME</div>
    <div class="col">EMAIL</div>
  </div>
  <div class="row d-flex flex-column mx-2 py-1 ">
    <div class="col">
      <input type="text" name="password" id="password"  value="<%= project.name%>" required disabled>

    </div>
    <div class="col">
      <input type="text" name="password" id="password" placeholder="Enter your password" value="<%= project.email%>" required disabled>

    </div>
  </div>
  <div class="row mx-2 py-1">
    <% if(project.delete === false){%>
    <button type="submit " class=" bg-transparent border-0"><b><a data-doc="<%=project._id%>" href="/userblock/<%=project._id %>" method="post" type="submit" class="btn-sub text-decoration-none text-light ">Block</a></b></button>
    <%} else {%>
      <button type="submit " class=" bg-transparent border-0"><b><a href="/userunblock/<%=project._id %>" type="button" class="btn-sub text-decoration-none text-light ">UnBlock</a></b></button>
      <%}%>
  </div>
</div>
<% })%>
<%} else {%>
<h1 class="text-center text-light">There Is No Users</h1>
<%}%>
</div>

    <footer>
    </footer>







    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-kenU1KFdBIe4zVF0s0G1M5b4hcpxyD9F7jL+jjXkk+Q2h455rYXK/7HAuoJl+0I4" crossorigin="anonymous"></script>

</body>

</html>


